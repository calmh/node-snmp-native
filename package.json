{
  "name": "snmp-node",
  "description": "A native Javascript SNMP implementation for Node.js",
  "author": "Emre Acar <emreacar@hotmail.com>",
  "keywords": [
    "snmp",
    "asn.1",
    "asn1",
    "network",
    "monitoring"
  ],
  "homepage": "https://github.com/neias/snmp-node",
  "version": "1.2.0",
  "license": "MIT",
  "main": "lib/snmp.js",
  "scripts": {
    "test": "NODE_PATH=lib mocha -R spec",
    "hint": "jshint *.js lib/*.js",
    "doc": "docco lib/* example.js 2>/dev/null",
    "cov": "jscoverage lib lib-cov && EXPRESS_COV=1 NODE_PATH=lib-cov mocha -R html-cov > docs/coverage.html"
  },
  "dependencies": {},
  "devDependencies": {
    "docco": "~0.8.0",
    "jscoverage": "~0.6.0",
    "jshint": "~2.11.1",
    "mocha": "~7.2.0",
    "should": "~13.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/neias/snmp-node.git"
  },
  "bugs": {
    "url": "https://github.com/neias/snmp-node/issues",
    "email": "emreacar@hotmail.com"
  }
}
